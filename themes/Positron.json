{
  "$schema": "vscode://schemas/color-theme",
  "author": "Mirko Schubert",
  "name": "Positron",
  "type": "dark",
  "colorSpaceName": "sRGB",
  "semanticClass": "theme.dark.positron",
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6f7878",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#34bc6e"
      }
    },
    {
      "scope": "keyword.control, keyword.operator.new",
      "settings": {
        "foreground": "#2d74da"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#d0dada"
      }
    },
    {
      "scope": "keyword.operator.logical, keyword.operator.comparison",
      "settings": {
        "foreground": "#71cddd"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#34bc6e"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#ffb000"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#d0dada"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#d0dada"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#424747"
      }
    },
    {
      "scope": "meta.object-binding-pattern-variable.js variable",
      "settings": {
        "foreground": "#c4881c"
      }
    },
    {
      "scope": "variable.other.constant, variable.language.this, variable.interpolation",
      "settings": {
        "foreground": "#d0dada"
      }
    },
    {
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#34bc6e"
      }
    },
    {
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#d0dada"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "border-bottom": "1px dashed rgba(255, 44, 109, 0.5)"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "background-foreground": "rgba(255, 44, 109, 0.62)"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#d0dada"
      }
    },
    {
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#c4881c"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#34bc6e"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#34bc6e"
      }
    },
    {
      "scope": "support.type.object.module.js",
      "settings": {
        "foreground": "#ff509e"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#95d13c"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#95d13c"
      }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#f87eac"
      }
    },
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#34bc6e"
      }
    },
    {
      "scope": "meta.decorator punctuation.decorator",
      "settings": {
        "foreground": "#ffb000"
      }
    },
    {
      "scope": "meta.decorator variable",
      "settings": {
        "foreground": "#71cddd"
      }
    },
    {
      "scope": "keyword.other.special-method",
      "settings": {
        "foreground": "#2d74da"
      }
    },
    {
      "scope": "keyword.control.at-rule",
      "settings": {
        "foreground": "#34bc6e"
      }
    },
    {
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#ff806c"
      }
    },
    {
      "scope": "variable.interpolation",
      "settings": {
        "foreground": "#34bc6e"
      }
    }
  ],
